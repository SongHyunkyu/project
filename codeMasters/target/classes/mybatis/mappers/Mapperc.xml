<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.dao.ContactFormDAO"> <!-- "com.example.dao.id가 dao에서 실행 되는 것이다. -->
	<insert id="insertContactForm"
		parameterType="com.codemasters.project.dto.ContactFormDTO">
		INSERT INTO contactus (title, content, date) VALUES (#{title}, #{content},
		#{date})
	</insert>
</mapper>